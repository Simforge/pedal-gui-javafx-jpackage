<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="266.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.clutch.ClutchController">
    <children>
        <Label layoutX="120.0" text="Clutch">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <LineChart fx:id="clutchChart" layoutY="245.0" prefHeight="225.0" prefWidth="250.0" stylesheets="@../style.css">
            <xAxis>
                <NumberAxis side="BOTTOM" fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" layoutX="100.0" side="LEFT" />
            </yAxis>
        </LineChart>
        <Label layoutX="60.0" layoutY="46.0" text="0%" />
        <Label layoutX="52.0" layoutY="75.0" text="20%" />
        <Label layoutX="52.0" layoutY="104.0" text="40%" />
        <Label layoutX="52.0" layoutY="133.0" text="60%" />
        <Label layoutX="53.0" layoutY="162.0" text="80%" />
        <Label layoutX="47.0" layoutY="191.0" text="100%" />
        <TextField fx:id="input_0" layoutX="88.0" layoutY="42.0" />
        <TextField fx:id="input_20" layoutX="88.0" layoutY="71.0" />
        <TextField fx:id="input_40" layoutX="88.0" layoutY="100.0" />
        <TextField fx:id="input_60" layoutX="88.0" layoutY="129.0" />
        <TextField fx:id="input_80" layoutX="88.0" layoutY="158.0" />
        <TextField fx:id="input_100" layoutX="88.0" layoutY="187.0" />
        <Button layoutX="139.0" layoutY="224.0" mnemonicParsing="false" onAction="#handleAction" text="save to arduino" />
    </children>
</AnchorPane>
